<% if Exercise.where('created_at < ?', exercise.created_at).where(movement_id: exercise.movement_id).last %>
    <div class="last_performance">
            <div class="time_ago">
                <span class="material-symbols-outlined">history</span>
                <p><%= time_ago_in_words(l Exercise.where('created_at < ?', exercise.created_at).where(movement_id: exercise.movement_id).last.created_at) %></p>
            </div>
        <ul>
            <% Serie.where(exercise_id: Exercise.where('created_at < ?', exercise.created_at).where(movement_id: exercise.movement_id).last.id).each do |last_serie| %>
                <li class="small_text"><%= last_serie.weight_in_preferred_unit %> x <%= last_serie.repetition %></li>
            <% end %>
        </ul>
    </div>
<% end %>


<%= turbo_stream_from "workouts" %>
<main class="container">
    <div class="header">
        <h1>Workouts</h1>
    </div>

    <%= turbo_frame_tag Workout.new %>

    <%= turbo_frame_tag "workouts" do %>
        <%= render @workouts %>
    <% end %>
</main>
<footer class="footer">
        <%= link_to "New Workout",
            new_workout_path,
            class: "btn btn--primary",
            data: { turbo_frame: dom_id(Workout.new) } %>
</footer>

